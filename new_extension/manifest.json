{
  "manifest_version": 2,

  "name": "Audio File Upload",
  "description": "This extension allows a user to upload an audio news reading.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "upload.html",
	"default_title": "Click here!"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}